<%= "Create a Blogger"%>
<%= render 'layouts/flash'%>
<%=form_for @blogger do |f|%>
    <%= f.label :name%>
    <%= f.text_field :name%>
    <br>
    <%= f.label :bio%>
    <%= f.text_area :bio%>
    <br>
    <%= f.label :age%>
    <%= f.number_field :age%>
    <br>
    <%= f.submit "Create Blogger"%>
<%end%>